{
	"manifest_version": 3,
	"name": "Sellpy List Total Calculator",
	"version": "1.0",
	"description": "Sums all [itemprop='price'] content attributes on the page.",
	"permissions": ["activeTab", "scripting"],
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "/assets/images/icon-16.png",
			"48": "/assets/images/icon-48.png",
			"128": "/assets/images/icon-128.png"
		}
	},
	"icons": {
		"16": "/assets/images/icon-16.png",
		"48": "/assets/images/icon-48.png",
		"128": "/assets/images/icon-128.png"
	}
}
